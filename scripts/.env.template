# Ansible Automation Platform Credentials
CONTROLLER_HOST="your-aap-hostname"
CONTROLLER_USERNAME="your-username"
CONTROLLER_PASSWORD="your-password"

# OpenShift Configuration
KUBECONFIG="/path/to/your/kubeconfig"

# Optional SSL Configuration
CONTROLLER_VERIFY_SSL="false"

# Optional Debug Settings
ANSIBLE_DEBUG="false"
ANSIBLE_VERBOSE=""  # Set to "-v", "-vv", or "-vvv" for different verbosity levels

# Optional Collection Path
ANSIBLE_COLLECTIONS_PATH="/path/to/collections" 